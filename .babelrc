{
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-react-jsx"
  ],
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "since 2012"
          ]
        },
        "debug": true,
        "modules": false,
        "loose": true
      }
    ]
  ]
}
